
REGION=ap-northeast-1
STAGE=dev
PROFILE=serverless-circleci_dev

slss project remove

sleep 10



AWSDIR=~/.aws
mkdir -p ${AWSDIR}
CREDENTIALS=${AWSDIR}/credentials

echo "[${PROFILE}]" > ${CREDENTIALS}
echo "aws_access_key_id=${AWS_ACCESS_KEY_ID}" >> ${CREDENTIALS}
echo "aws_secret_access_key=${AWS_SECRET_ACCESS_KEY}" >> ${CREDENTIALS}

slss project init -p ${PROFILE} -s ${STAGE} -r ${REGION}

