
echo "AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID}" > admin.env
echo "AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY}" >> admin.env

REGION=ap-northeast-1
PROJECT=circleci
STAGE=dev

slss project remove

aws --region ${REGION} cloudformation describe-stacks --stack-name ${PROJECT}-${STAGE}-r
sleep 10

slss project init -p dummy -s ${STAGE} -r ${REGION}

