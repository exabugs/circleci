
REGION=ap-northeast-1
STAGE=dev
PROFILE=serverless-circleci_dev

slss project remove

sleep 10



AWSDIR=~/.aws
mkdir -p ${AWSDIR}
CREDENTIALS=${AWSDIR}/credentials

cat << EOS > ${CREDENTIALS}
[${PROFILE}]
aws_access_key_id=${AWS_ACCESS_KEY_ID}
aws_secret_access_key=${AWS_SECRET_ACCESS_KEY}
EOS

slss project init -p ${PROFILE} -s ${STAGE} -r ${REGION}

